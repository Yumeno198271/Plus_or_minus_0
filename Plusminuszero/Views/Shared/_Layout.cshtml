    <link href="~/Assets/css/loader-1.css" rel="stylesheet" />
    <link href="~/Assets/css/main.css" rel="stylesheet" />
    <link href="~/Assets/css/responsive.css" rel="stylesheet" />

    <link rel="icon" href="~/favicon.ico" />


    <script src="~/Assets/js/vendor/modernizr-2.8.3-respond-1.4.2.min.js"></script>
</head>
<body>








    <!-- Preloader -->
    <div class="cover"></div>

    <div class="header">
        <div class="container">
            <div class="logo">
                <a href="@Url.Content("~/Homepage/Index")">
                    <img src="~/Assets/img/logo.png" alt="Logo" />
                </a>
            </div>

            <!-- Menu Hamburger (Default) -->
            <button class="main-menu-indicator" id="open-button">
                <span class="line"></span>
            </button>
            <div class="menu-wrap" style="background-image: url(Assets/img/nav_bg.jpg)">
                <div class="menu-content">
                    <div class="navigation">
                        <input type="button" class="pe-7s-close close-menu" id="close-button" value="×" width:55px;height:55px" style="color: #ffffff;background-color:black;border:none;">
                        <div class="search-wrap js-ui-search">
                            <input class="js-ui-text" type="text" placeholder="Search Here...">
                            <span class="eks js-ui-close"></span>
                        </div>
                    </div>
                    <nav class="menu">
                        <div class="menu-list">
                            <ul>
                                <li class="menu-item-has-children">
                                    <a href="@Url.Content("~/Homepage/Index")">Home</a>
                                    <ul class="sub-menu">
                                        <li><a href="@Url.Content("~/Homepage/Index")">- Default</a></li>
                                        <li><a href="@Url.Content("~/Homepage/Homepage_Architecture")">- Architecture</a></li>
                                        <li><a href="@Url.Content("~/Homepage/Slider_Full_Width")">- Slider Full Width</a></li>
                                    </ul>
                                </li>
                                <li><a href="@Url.Content("~/About/Index")">About</a></li>
                                <li class="menu-item-has-children">
                                    <a href="#">News</a>
                                    <ul class="sub-menu">
                                        <li><a href="@Url.Content("~/News/Event_News")">-Event News</a></li>
                                        <li><a href="@Url.Content("~/News/Product_Information")">-Product Information</a></li>
                                        <li><a href="@Url.Content("~/News/Blog_Single")">Blog Single</a></li>
                                    </ul>
                                </li>
                                <li class="menu-item-has-children">
                                    <a href="@Url.Content("~/Portfolio/Portfolio_Masonry")">product</a>
                                    <ul class="sub-menu">
                                        <li><a href="@Url.Content("~/Portfolio/Portfolio_Masonry")">-Portfolio Masonry</a></li>
                                        <li><a href="@Url.Content("~/Portfolio/Portfolio_Dribbble")">-Portfolio Dribbble</a></li>
                                        <li><a href="@Url.Content("~/Portfolio/Welfare_Goods")">-Welfare goods</a></li>
                                        <li><a href="@Url.Content("~/Portfolio/Dribbble")">-Portfolio Dribbble</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </nav>

                    <div class="hidden-xs">
                        <div class="menu-social-media">
                            <ul>
                                <li><a href="#"><img src="~/Assets/img/icon/icon_facebook.ico" class="iconmoon-facebook" style="width:30px;height:30px" /></a></li>
                                <li><a href="#"><img src="~/Assets/img/icon/icon_twitter.ico" class="iconmoon-twitter" style="width:30px;height:30px" /></a></li>
                            </ul>
                        </div>

                        <div class="menu-information">
                            <ul>
                                <li><span>T:</span> 003 124 115</li>
                                <li><span>E:</span> info@mail.com</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End of Menu Hamburger (Default) -->

        </div>
    </div>






















    <div class="container body-content">
        @RenderBody()
    </div>


    <div class="footer margin-top">
        <div class="container">
            <div class="row">
                <div class="col-md-2 col-sm-4 col-xs-12">
                    <div class="footer-inner">
                        <div class="footer-content">
                            <h4>O.A.K THEME</h4>
                            <address>City 35 AM <br>Street 1395 p.n <br>your Country</address>
                        </div>
                    </div>
                </div>

                <div class="col-md-2 col-md-push-8 col-sm-4 col-xs-12">
                    <div class="footer-inner">
                        <div class="footer-content">
                            <h4>CONTACT INFO</h4>
                            <p>
                                T:003 124 115 <br> E:info@mail.com <br> W:www.website.com
                            </p>
                        </div>
                    </div>
                </div>

                <div class="col-md-4 col-sm-4 col-xs-12">
                    <div class="footer-inner">
                        <div class="footer-content">
                            <ul class="social-media">
                                <li><a href="#"><i class="iconmoon-facebook"></i></a></li>
                                <li><a href="#"><i class="iconmoon-twitter"></i></a></li>
                                <li><a href="#"><i class="iconmoon-flickr2"></i></a></li>
                                <li><a href="#"><i class="iconmoon-dribbble3"></i></a></li>
                                <li><a href="#"><i class="iconmoon-pinterest"></i></a></li>
                                <li><a href="#"><i class="iconmoon-linkedin2"></i></a></li>
                            </ul>
                            <span class="copyright-mark">&copy; 2015 OAK, ALL RIGHTS RESERVED</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    @RenderSection("endCSS", required: false)
    @RenderSection("endJS", required: false)

    <script src="~/Assets/js/vendor/jquery-1.11.2.min.js"></script>
    <script src="~/Assets/js/vendor/pace.min.js"></script>
    <script src="~/Assets/js/vendor/bootstrap.min.js"></script>
    <script src="~/Assets/js/vendor/classie.js"></script>
    <script src="~/Assets/js/vendor/isotope.pkgd.min.js"></script>
    <script src="~/Assets/js/vendor/jquery.velocity.min.js"></script>
    <script src="~/Assets/js/vendor/jquery.kenburnsy.min.js"></script>
    <script src="~/Assets/js/vendor/textslide.js"></script>
    <script src="~/Assets/js/vendor/imagesloaded.pkgd.min.js"></script>
    <script src="~/Assets/js/vendor/tabs.js"></script>
    <script src="~/Assets/js/ef-slider.js"></script>
    <script src="~/Assets/js/vendor/owl.carousel.min.js"></script>
    <script src="~/Assets/js/vendor/jquery.magnific-popup.min.js"></script>
    <script src="~/Assets/js/vendor/jquery.social-buttons.min.js"></script>
    <script src="~/Assets/js/vendor/wow.min.js"></script>
    <script src="~/Assets/js/main.js"></script>
    <script src="~/Assets/js/ajax.js"></script>
</body>
</html>
